<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:ScheduleTracker.ViewModels;assembly=ScheduleTracker"
        x:Class="ScheduleTracker.Views.MainWindow"
        Title="ScheduleTracker2">
	<StackPanel>
		<TextBlock Text="Podaj nazwę użytkownika:"/>
		<TextBox Text="{Binding UserName}"/>
		<Button Command="{Binding GreetCommand}" Content="Witaj!"/>
		<TextBlock Text="{Binding GreetingMessage}" Margin="0,10,0,0"/>
	</StackPanel>
</Window>
